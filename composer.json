{
    "name": "compwright/oauth2-housecallpro",
    "description": "Housecall Pro OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathon Hill",
            "email": "jonathon@compwright.com",
            "homepage": "https://compwright.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "housecallpro",
        "provider"
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "league/oauth2-client": "^2.5",
        "psr/http-message": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "squizlabs/php_codesniffer": "^3.6",
        "psr/http-message": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "CompWright\\OAuth2\\HousecallPro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CompWright\\OAuth2\\HousecallPro\\": "test/"
        }
    },
    "scripts": {
        "test": [
            "phpcs --standard=psr2 src/",
            "phpunit"
        ]
    }
}
